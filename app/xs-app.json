{
	"welcomeFile": "/app/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/app/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	},
	{
		"source": "/srv-binding/(.*)$",
		"target": "$1",
		"authenticationType": "xsuaa",
		"destination": "srv-binding",
		"csrfProtection": false
	  }
	]
}